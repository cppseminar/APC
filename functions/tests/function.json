{
  "scriptFile": "tests.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "tests/{id?}",
      "methods": [
        "get",
        "post",
        "patch"
      ]
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "queue",
      "queueName": "taskQueue",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
